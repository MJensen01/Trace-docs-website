{
  "name": "trace-docs-website",
  "version": "1.0.0",
  "description": "Website for Trace Docs Product",
  "main": "index.js",
  "scripts": {
    "css:build": "npx @tailwindcss/cli -i src/css/input.css -o src/css/output.css --minify",
    "11ty:build": "npx @11ty/eleventy",
    "build": "npm run css:build && npm run 11ty:build",
    "css:dev": "npx @tailwindcss/cli -i src/css/input.css -o src/css/output.css --watch",
    "11ty:dev": "npx @11ty/eleventy --serve",
    "dev": "npm run css:build && concurrently \"npm run css:dev\" \"npm run 11ty:dev\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@tailwindcss/cli": "^4.1.18",
    "tailwindcss": "^4.1.18"
  }
}
